<h2>New Post</h2>

<%= form_for @post do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :topic %>
  <%= f.text_field :topic %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :text %>
  <%= f.text_field :text %>

  <%= f.submit "Create" %>
  <% end %>
